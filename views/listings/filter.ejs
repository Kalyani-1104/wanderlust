<style>
  #filters {
    display: flex;
    flex-wrap: wrap;
  }

  .filter {
    margin-top: 15px;
    margin-left: 20px;
    margin-bottom: 0px;
  }

  .filter div {
    text-align: center;
    opacity: 0.5;
  }

  .filter svg {
    width: 35px;
    height: 35px;
  }

  .filter div:hover {
    opacity: 1;
  }

  .toggle_tax {
    border: 2px solid #302c2c;
    border-radius: 1.5rem;
    height: 3.5rem;
    width: 12.5rem;
    padding: 0.75rem 1rem;
    margin: 1rem;
    color: #222;
    font-weight: 500;
    display: flex;
    align-items: center;
    justify-content: center;
    cursor: pointer;
    transition: all 0.3s ease;
  }

  .toggle_tax:hover {
    background-color: #e6e6e6;
    border-color: #1f1f1f;
  }

  a {
    color: #000000;
    text-decoration: none;
  }

  /* Mobile styles */
  /* Hide button by default */
  #showFiltersBtn {
    display: none;
  }

  /* Default filters (desktop view) */
  #filters {
    display: flex;
    flex-wrap: wrap;
  }

  /* Mobile styles */
  @media (max-width: 600px) {
    #filters {
      display: none; /* hidden by default */
      flex-wrap: wrap;
      margin-top: 1rem;
    }

    #filters.show-all {
      display: flex; /* show when button clicked */
    }

    /* Show button only on mobile */
    #showFiltersBtn {
      display: block;
    }

    .filter {
      flex: 0 0 auto;
      min-width: 70px;
      margin: 0.5rem;
    }
  }
</style>

