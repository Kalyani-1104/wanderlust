<script>
  let taxSwitch = document.getElementById("checkNativeSwitch");
  taxSwitch.addEventListener("click", () => {
    let tax_toggle = document.getElementsByClassName("tax-toggle");
    console.log(tax_toggle);
    for (tax of tax_toggle) {
      if (tax.style.display != "inline") {
        tax.style.display = "inline";
      } else {
        tax.style.display = "none";
      }
    }
  });

  // Toggle filters on button click
  document
    .getElementById("showFiltersBtn")
    .addEventListener("click", function () {
      const filters = document.getElementById("filters");
      filters.classList.toggle("show-all");

      // Change button text dynamically
      if (filters.classList.contains("show-all")) {
        this.textContent = "Hide Filters";
      } else {
        this.textContent = "Show All Filters";
      }
    });
</script>



